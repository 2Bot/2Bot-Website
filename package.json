    "license": "MIT",
    "private": true,
    "devDependencies": {
        "@types/hapi": "^17.0.10",
        "@types/node-fetch": "^1.6.8",
        "concurrently": "^3.5.1",
        "nodemon": "^1.17.3",
        "typescript": "^2.8.1"
    }
}
