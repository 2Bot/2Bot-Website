{
    "scripts": {
        "start": "concurrently \"npm run start-client\" \"npm run start-server\"",
        "start-client": "yarn --cwd client/ start",
        "start-js": "nodemon --delay 1 build/server.js",
        "watch-ts": "tsc -w",
        "start-server": "concurrently \"npm run watch-ts\" \"npm run start-js\""
    },
